            {
              "id": "2c1d4689-5f17-423f-986f-af6490461f39",
              "mealType": "BREAKFAST",
              "recipeId": "b2735d49-4f9a-4805-ac3c-eb699496d27c",
              "dayId": "f2e14dfb-5f4d-4c40-ab08-a9114e4fdce4",
