            {
              "id": "5066aa60-4eaa-4d9e-b018-d67e95763967",
              "mealType": "LUNCH",
              "recipeId": "2710e9a8-efd5-426e-acbd-de48b53aaa2b",
              "dayId": "7c368733-5fbc-41b5-8d75-ed6ef15780ed",
